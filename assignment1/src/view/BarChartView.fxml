<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>

<BarChart fx:id="bartChartView" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <xAxis>
    <CategoryAxis fx:id="timeX" side="BOTTOM" />
  </xAxis>
  <yAxis>
    <NumberAxis fx:id="temperatureY" side="LEFT" />
  </yAxis>
</BarChart>
